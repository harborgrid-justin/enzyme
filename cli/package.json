{
  "name": "@missionfabric-js/enzyme-cli",
  "version": "1.0.0",
  "description": "Enterprise CLI scaffolding tool for the Enzyme React framework",
  "main": "dist/index.js",
  "bin": {
    "enzyme": "./dist/index.js",
    "enz": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "enzyme",
    "cli",
    "scaffolding",
    "react",
    "generator",
    "missionfabric"
  ],
  "author": "MissionFabric",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "handlebars": "^4.7.8",
    "glob": "^10.3.10",
    "zod": "^3.22.4",
    "yaml": "^2.3.4",
    "fs-extra": "^11.2.0",
    "execa": "^8.0.1",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^16.3.1",
    "semver": "^7.5.4",
    "update-notifier": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/semver": "^7.5.6",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ]
}
