{
  "name": "@missionfabric-js/enzyme-cli",
  "version": "1.0.0",
  "description": "Enterprise CLI scaffolding tool for the Enzyme React framework",
  "type": "module",
  "license": "MIT",
  "author": "Defendr Team",
  "homepage": "https://github.com/harborgrid-justin/enzyme-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harborgrid-justin/enzyme-cli.git"
  },
  "bugs": {
    "url": "https://github.com/harborgrid-justin/enzyme-cli/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "bin": {
    "enzyme": "./dist/index.js",
    "enz": "./dist/index.js"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist .tsbuildinfo .eslintcache",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint src --cache",
    "lint:fix": "eslint src --fix --cache",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "verify": "npm run typecheck && npm run lint && npm run test"
  },
  "keywords": [
    "enzyme",
    "cli",
    "scaffolding",
    "react",
    "generator",
    "missionfabric",
    "enterprise",
    "framework",
    "typescript"
  ],
  "author": "Defendr Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "handlebars": "^4.7.8",
    "glob": "^10.3.10",
    "zod": "^3.22.4",
    "yaml": "^2.3.4",
    "fs-extra": "^11.2.0",
    "execa": "^8.0.1",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^16.3.1",
    "semver": "^7.5.4",
    "update-notifier": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/semver": "^7.5.6",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "typescript-eslint": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.3.2"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false
}
